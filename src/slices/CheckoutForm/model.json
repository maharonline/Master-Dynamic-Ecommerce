          "type": "StructuredText",
          "config": {
            "single": "paragraph",
            "label": "Pay now label"
          }
        },
        "checkout_footer_notice": {
          "type": "StructuredText",
          "config": {
            "multi": "paragraph,hyperlink",
            "label": "Checkout footer notice"
          }
        },
        "shipping_icon": {
          "type": "Group",
          "config": {
            "label": "Shipping",
            "repeat": true,
            "fields": {
              "shipping_icon": {
                "type": "Image",
                "config": {
                  "label": "Shipping Icon",
                  "constraint": {},
                  "thumbnails": []
                }
              },
              "name": {
                "type": "Text",
                "config": {
                  "label": "name",
                  "placeholder": ""
                }
              }
            }
          }
        },
        "shipping_heading": {
          "type": "Text",
          "config": {
            "label": "Shipping Heading",
            "placeholder": ""
          }
        },
        "shipping_description": {
          "type": "Text",
          "config": {
            "label": "Shipping Description",
            "placeholder": ""
          }
        }
