          "config": {
            "single": "heading2",
            "label": "Contact title"
          }
        },
        "contact_email": {
          "type": "Text",
          "config": {
            "label": "Email"
          }
        },
        "contact_news_opt_in": {
          "type": "Boolean",
          "config": {
            "label": "Email me with news and offers"
          }
