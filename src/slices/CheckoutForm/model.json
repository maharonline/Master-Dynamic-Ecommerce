        "country": {
          "type": "Select",
          "config": {
            "label": "Country/Region",
            "options": [
              "United States",
              "Other"
            ]
          }
        },
        "first_name": {
          "type": "Text",
          "config": {
            "label": "First name"
          }
        },
        "last_name": {
          "type": "Text",
          "config": {
            "label": "Last name"
          }
        },
        "company": {
          "type": "Text",
          "config": {
            "label": "Company (optional)"
          }
        },
        "address": {
          "type": "Text",
          "config": {
            "label": "Address"
          }
        },
        "apartment_suite": {
          "type": "Text",
          "config": {
            "label": "Apartment, suite, etc. (optional)"
          }
