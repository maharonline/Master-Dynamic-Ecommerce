              "provider_logo": {
                "type": "Image",
                "config": {
                  "label": "Provider logo"
                }
              },
              "provider_name": {
                "type": "Text",
                "config": {
                  "label": "Provider name"
                }
              }
            }
          }
        },
        "show_more_options": {
          "type": "Boolean",
          "config": {
            "label": "Show more options"
